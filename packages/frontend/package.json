{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently \"yarn dev\" \"wait-on http://localhost:5709 && yarn start:electron\"",
    "start:electron": "electron .",
    "build:electron": "electron-builder build",
    "prestart:electron": "yarn build",
    "prebuild:electron": "yarn build",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "prepreview": "yarn build",
    "clean": "rm -rf dist && rm -rf build",
    "format": "prettier . --write",
    "predev": "yarn",
    "prebuild": "yarn && yarn clean"
  },
  "keywords": [],
  "author": "Ankit Kumar Pandit",
  "license": "ISC",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "hls.js": "^1.4.10",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-modal": "^3.16.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "redux-persist": "^6.0.0",
    "srt-webvtt": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-modal": "^3.16.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "^8.4.26",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.0"
  }
